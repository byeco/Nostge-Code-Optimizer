{
  "manifest_version": 3,
  "name": "OmniTab",
  "version": "0.5",
  "description": "OmniTab optimizes your codes, you can use the optimization function you want with the filter option",
  "background": {
    "serviceWorker": "JS/background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "48": "ICON/icon.png"
  },
  "action": {
    "default_icon": "ICON/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["JS/content.js"]
    }
  ]
}
